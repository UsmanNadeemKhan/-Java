
enum Laptop{
    macbook(2000),envy(1500),probook(2500);

private int price;

private Laptop(int price) {
    this.price = price;
}

public int getPrice() {
    return price;
}

public void setPrice(int price) {
    this.price = price;
}


}

public class hello{
    public static void main(String[] args) {
        for( Laptop lp : Laptop.values()){
            System.out.println(lp + " : " + lp.getPrice() );
        }
    }
}


