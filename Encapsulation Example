
// encapsulation example 
class human {

private int age ;
private String name ;    


    public int getage(){
        return age ;
    }
    
    public String getname(){
        return name ;
    }
 
    public void setage(int a){
        age = a;
    }

    public void setname(String n){
        name = n;
    }

}



public class hello{

    public static void main(String[] args){
        human obj = new human();
        obj.setage(50);
        obj.setname("usman");
        System.out.println(obj.getname());

        System.out.println(obj.getage());
    }
    }


